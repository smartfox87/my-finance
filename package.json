{
	"name": "my-finance",
	"version": "1.0.0",
	"type": "module",
	"private": true,
	"scripts": {
		"css": "node --loader ts-node/esm ./scripts/css.ts",
		"sitemap": "node --loader ts-node/esm ./scripts/sitemap.ts",
		"locales": "node --loader ts-node/esm ./scripts/locales.ts",
		"dev": "npx kill-port 3020 && npm run locales && next dev -p 3020",
		"build": "npm run locales && next build && npm run css",
		"start": "npx kill-port 3020 && next start -p 3020",
		"size": "npx cross-env ANALYZE=true npm run build",
		"typescript": "tsc --noEmit",
		"eslint": "eslint \"**/*.{js,jsx,ts,tsx}\"",
		"eslint:fix": "eslint \"**/*.{js,jsx,ts,tsx}\" --fix",
		"stylelint": "stylelint \"**/*.{css,scss,less,styl}\"",
		"cypress:open": "cypress open",
		"e2e": "cypress open --e2e",
		"e2e:headless": "cypress run --e2e",
		"component": "cypress open --component",
		"component:headless": "cypress run --component",
		"test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
		"test:watch": "node --experimental-vm-modules node_modules/jest/bin/jest.js --watch"
	},
	"dependencies": {
		"@ant-design/nextjs-registry": "^1.0.1",
		"@ducanh2912/next-pwa": "^10.2.9",
		"@reduxjs/toolkit": "^2.3.0",
		"@sentry/nextjs": "^8.38.0",
		"@supabase/supabase-js": "^2.46.1",
		"@vercel/speed-insights": "^1.1.0",
		"antd": "^5.22.1",
		"i18next": "^24.2.1",
		"i18next-resources-to-backend": "^1.2.1",
		"lodash": "^4.17.21",
		"next": "^14.2.13",
		"next-i18n-router": "^5.5.1",
		"nodemailer": "^6.9.16",
		"query-string": "^9.1.1",
		"react": "^18",
		"react-dom": "^18",
		"react-google-recaptcha": "^3.1.0",
		"react-i18next": "^15.1.1",
		"react-loader-spinner": "^6.1.6",
		"react-redux": "^9.1.2",
		"recharts": "^2.13.3",
		"redux": "^5.0.1"
	},
	"devDependencies": {
		"@cypress/webpack-preprocessor": "^6.0.2",
		"@faker-js/faker": "^9.2.0",
		"@next/bundle-analyzer": "^15.0.3",
		"@svgr/webpack": "^8.1.0",
		"@testing-library/dom": "^10.4.0",
		"@testing-library/jest-dom": "^6.6.3",
		"@testing-library/react": "^16.0.1",
		"@types/fs-extra": "^11.0.4",
		"@types/jest": "^29.5.13",
		"@types/lodash": "^4.17.13",
		"@types/node": "^22",
		"@types/nodemailer": "^6.4.16",
		"@types/react": "^18.3.12",
		"@types/react-dom": "^18",
		"@types/react-google-recaptcha": "^2.1.9",
		"@types/recharts": "^1.8.29",
		"@typescript-eslint/eslint-plugin": "^6.21.0",
		"@typescript-eslint/parser": "^6.21.0",
		"autoprefixer": "^10.4.20",
		"cheerio": "^1.0.0",
		"cypress": "^13.15.2",
		"cypress-terminal-report": "^7.0.4",
		"dotenv": "^16.4.5",
		"eslint": "^8",
		"eslint-config-next": "15.0.3",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-jest": "^28.9.0",
		"eslint-plugin-prettier": "^5.2.1",
		"i18nexus-cli": "^3.5.0",
		"jest": "^29.7.0",
		"jest-environment-jsdom": "^29.7.0",
		"postcss": "^8",
		"prettier": "^3.3.3",
		"prettier-plugin-tailwindcss": "^0.6.8",
		"schema-dts": "^1.1.2",
		"sitemap": "^8.0.0",
		"stylelint": "^16.10.0",
		"stylelint-prettier": "^5.0.2",
		"tailwindcss": "^3.4.14",
		"ts-essentials": "^10.0.3",
		"ts-jest": "^29.2.5",
		"ts-loader": "^9.5.1",
		"ts-node": "^10.9.2",
		"typescript": "^5",
		"webpack": "^5.96.1"
	}
}
